
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/DashboardLayout/_DashboardLayout.cshtml";
}


<div class="container">
    <div class="page-inner">
        <div class="d-flex align-items-left align-items-md-center flex-column flex-md-row pt-2 pb-4">
            <div>
                <h3 class="fw-bold mb-3">Kişiselleştirme Alanı</h3>
                <h6 class="op-7 mb-2">Acunmedya Akademi Menü Projesi</h6>
            </div>

        </div>
        <div class="row">
            @await Component.InvokeAsync("_WeatherDashboardComponentPartial")
            @await Component.InvokeAsync("_ForeignCurrencyDashboardComponentPartial")

        </div>
        @await Component.InvokeAsync("_SocialApiDashboardComponentPartial", new { username = "ipekoztrk" })


        @await Component.InvokeAsync("_MediumDashboardComponentPartial")


    </div>
